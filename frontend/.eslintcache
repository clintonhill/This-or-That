header'>\n        Browse Questions\n      </div>\n      <div>\n        {loadedQuestions && loadedQuestions.map((question, idx) => {\n          { if(idx < 15) return <QuestionRow question={question}/>}\n      })}\n      </div>\n      <div className='pagination'>\n      <button onClick={()=> setPage(prev => prev - 1)} disabled={page <= 1}><FontAwesomeIcon icon={faAngleDoubleLeft} size='lg'/></button>\n      <button onClick={()=> setPage(prev => prev + 1)} disabled={!loadedQuestions || loadedQuestions[15] === undefined}><FontAwesomeIcon icon={faAngleDoubleRight} size='lg'/></button>\n      </div>\n    </div>\n  )\n}\n\nexport default BrowseQuestionsPage;\n","/home/clint/aa-projects/thisorthat/frontend/src/components/BrowseQuestionsPage/QuestionRow.js",["260"],"import { FontAwesomeIcon } from '@fortawesome/react-fontawesome'\nimport { faCheckCircle, faPenFancy } from '@fortawesome/free-solid-svg-icons'\nimport { useHistory } from 'react-router-dom'\n\nexport default function QuestionRow({question}) {\n  const history = useHistory();\n  const sendToQuestionPage = () => {\n    history.push(`/questions/${question.id}`)\n  }\n  return (\n    <div className='question-row__main' onClick={sendToQuestionPage}>\n      {question && question.title} <span className='isAnswered'> <FontAwesomeIcon icon={faPenFancy} size='lg'/></span>\n    </div>\n  )\n}\n",{"ruleId":"261","replacedBy":"262"},{"ruleId":"263","replacedBy":"264"},{"ruleId":"261","replacedBy":"265"},{"ruleId":"263","replacedBy":"266"},{"ruleId":"261","replacedBy":"267"},{"ruleId":"263","replacedBy":"268"},{"ruleId":"269","severity":1,"message":"270","line":3,"column":10,"nodeType":"271","messageId":"272","endLine":3,"endColumn":18},{"ruleId":"269","severity":1,"message":"273","line":6,"column":9,"nodeType":"271","messageId":"272","endLine":6,"endColumn":16},{"ruleId":"269","severity":1,"message":"274","line":1,"column":10,"nodeType":"271","messageId":"272","endLine":1,"endColumn":19},{"ruleId":"269","severity":1,"message